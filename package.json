{
  "name": "ShaderPark",
  "version": "0.0.1",
  "description": "",
  "main": "./client/index.html",
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "gcloud": {
    "id": "shader-park",
    "version": "0-0-9",
    "devPort": 3000,
    "devAdminPort": 8000
  },
  "scripts": {
    "qDeploy": "gcloud app deploy --project $npm_package_gcloud_id --version $npm_package_gcloud_version --no-promote",
    "deploy": "npm run build && gcloud app deploy --project $npm_package_gcloud_id --version $npm_package_gcloud_version",
    "browse": "gcloud app browse --project $npm_package_gcloud_id",
    "build": "parcel build ./client/index.html",
    "less": "lessc less/style.less client/style.css",
    "dev": "parcel ./client/index.html"
  },
  "author": "Peter Whidden, Torin Blankensmith, Andrew Palmer",
  "license": "Apache-2.0",
  "dependencies": {
    "@tweenjs/tween.js": "^17.2.0",
    "animejs": "^3.0.1",
    "dat.gui": "^0.7.6",
    "firebase": "^7.4.0",
    "intersection-observer": "^0.7.0",
    "three": "^0.103.0",
    "v-lazy-image": "^1.3.2",
    "vue": "^2.6.11",
    "vue-codemirror": "^4.0.6",
    "vue-hot-reload-api": "^2.3.4",
    "vue-js-modal": "^1.3.33",
    "vue-router": "^3.0.1",
    "vuelidate": "^0.7.4",
    "vuetify": "^2.2.12",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-regenerator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0",
    "@vue/babel-preset-jsx": "^1.1.2",
    "@vue/component-compiler-utils": "^3.1.1",
    "less": "^2.7.3",
    "parcel-bundler": "^1.12.4",
    "vue-template-compiler": "^2.6.11"
  }
}
